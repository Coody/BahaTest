class Main{
  static void inArray(String[]arr, String q[]){
    int counter = 0;
    for (int i = 0; i < arr.length; i++){
      if (q.equals(arr[i]))//這行似乎變得不會是true了
        counter++;  
    }
    if(counter!=0)
      System.out.print("True");
    else
      System.out.print("False");
  }
  public static void main(String[] args) {
    String[] arr = {"apple","banana","lemon"};
    String[] q   = {"apple"};
    inArray(arr, q);
  }
}
